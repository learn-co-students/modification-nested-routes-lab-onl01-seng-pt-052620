<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= artist_url(@song) %>
  <%= artist_url(:artist_id) %><br>
  <%= f.select :artist_id, options_from_collection_for_select(Artist.all, :id, :name) %><br>
  <%= f.hidden_field :artist_id %>

  <%= f.submit %>
<% end %>
